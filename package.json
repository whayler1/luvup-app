{
  "name": "Luvup",
  "description": "Improving relationships through gamification",
  "version": "0.0.24",
  "private": true,
  "devDependencies": {
    "babel-jest": "^22.0.6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint-config-react-native-prettier": "^1.0.1",
    "jest-expo": "^27.0.0",
    "prettier": "^1.13.7",
    "react-native-scripts": "1.7.0",
    "react-test-renderer": "16.0.0-beta.5"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-native-scripts start",
    "debug": "REACT_DEBUGGER=\"unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?port=19001' ||\" yarn start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "postinstall": "rndebugger-open",
    "devtools": "react-devtools",
    "lint": "eslint .",
    "prettier": "eslint . --fix"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "!node_modules/react-runtime"
    ],
    "setupFiles": [
      "./__MOCKS__/globalMock.js"
    ]
  },
  "dependencies": {
    "analytics-react-native": "^1.2.0",
    "art": "^0.10.1",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "color": "^3.0.0",
    "eslint": "^5.1.0",
    "expo": "^27.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "react": "16.3.1",
    "react-devtools": "^3.2.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-27.0.0.tar.gz",
    "react-native-debugger-open": "^0.3.17",
    "react-native-elements": "^0.18.5",
    "react-native-keyboard-aware-scroll-view": "^0.4.1",
    "react-native-router-flux": "^4.0.0-beta.27",
    "react-native-svg": "^6.1.3",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "1.5.11",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.8.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
