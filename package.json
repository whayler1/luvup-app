{
  "name": "Luvup",
  "description": "Stay together",
  "version": "0.0.30",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.9.0",
    "eslint-config-react-native": "^4.0.0",
    "eslint-config-react-native-prettier": "^1.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "isomorphic-fetch": "^2.2.1",
    "jest-expo": "^31.0.0",
    "prettier": "^1.15.3",
    "react-native-scripts": "^2.0.1",
    "react-test-renderer": "^16.6.3"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "expo start",
    "eject": "expo eject",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "test": "node node_modules/jest/bin/jest.js",
    "debug": "REACT_DEBUGGER=\"unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?port=19001' ||\" yarn start",
    "postinstall": "rndebugger-open",
    "devtools": "react-devtools",
    "lint": "eslint .",
    "prettier": "eslint . --fix"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "!node_modules"
    ],
    "globals": {
      "fetch": true
    },
    "verbose": true,
    "rootDir": "./",
    "setupFiles": [
      "<rootDir>/src/__MOCKS__/globalMocks.js"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^8.0.0",
    "analytics-react-native": "^1.2.0",
    "art": "^0.10.3",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "bezier-easing": "^2.1.0",
    "color": "^3.0.0",
    "date-fns": "^1.29.0",
    "eslint-plugin-babel": "^5.1.0",
    "expo": "^31.0.6",
    "expo-font": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.5.0",
    "react-devtools": "^3.2.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-native-debugger-open": "^0.3.17",
    "react-native-elements": "^0.18.5",
    "react-native-keyboard-aware-scroll-view": "^0.4.1",
    "react-native-router-flux": "^4.0.0-beta.27",
    "react-navigation": "1.5.11",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.8.3"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  }
}
